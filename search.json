---
---
[
    {%- for item in site.prayers %}
    {
        "title": "{{ item.title | escape }}",
        "url": "{{ item.url | relative_url }}",
        "lead": "{{ item.lead | strip_html }}",
        "era": "{{ item.era }}",
        "tags": "{{ item.tags | join: ',' }}",
        "date": "{{ item.date }}",
        "datePretty": "{{ item.date | date: "%b %d, %Y" }}"
    } {% unless forloop.last %},{% endunless -%}
    {% endfor -%}
]
